{
  "name": "advent-reading-challenge",
  "version": "1.0.0",
  "private": false,
  "description": "",
  "repository": "https://github.com/Renddslow/advent-reading-challenge",
  "license": "MIT",
  "author": "Matt McElwee <mubatt@wyopub.com>",
  "main": "index.js",
  "scripts": {
    "//": "Do not run on push",
    "build": "node scripts/build.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md,ts,tsx}": [
      "prettier --write"
    ]
  },
  "prettier": "@dmsi/prettier-config",
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "concurrency": 4,
    "extensions": [
      "ts"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "timeout": "1m"
  },
  "devDependencies": {
    "@dmsi/prettier-config": "^1.3.0",
    "ava": "^3.13.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "papaparse": "^5.3.0",
    "pupa": "^2.1.1"
  }
}
